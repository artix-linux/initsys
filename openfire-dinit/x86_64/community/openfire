type            = process
command         = /usr/bin/java -server -DopenfireHome=/var/lib/openfire -Dopenfire.lib.dir=/usr/lib/openfire -classpath /usr/lib/openfire/startup.jar -jar /usr/lib/openfire/startup.jar
smooth-recovery = true
logfile         = /var/log/dinit/openfire.log
waits-for       = loginready
